# Database Configuration
JDBC_DATABASE_URL=jdbc:postgresql://localhost:5544/hiresync_db
JDBC_DATABASE_USERNAME=hiresync_user
JDBC_DATABASE_PASSWORD=hiresync_password

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-should-be-at-least-256-bits-long
JWT_EXPIRATION=86400000

# Application Configuration
PORT=8080
SPRING_PROFILES_ACTIVE=prod

# Database Pool Configuration
DB_MAX_POOL_SIZE=10
DB_MIN_IDLE=5

# Logging Levels
LOG_LEVEL_ROOT=INFO
LOG_LEVEL_APP=INFO
LOG_LEVEL_SQL=INFO 